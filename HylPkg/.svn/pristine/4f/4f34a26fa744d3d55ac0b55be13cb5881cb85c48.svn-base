#ifdef CONTROL_DEFINITION  

#define SHOWPCINETWORK\
        oneof varid = SETUP_DATA.PcideviceDisplay,\
        questionid = AUTO_ID(SHOW_PCI_NETWORK),\
        prompt  = STRING_TOKEN(STR_SETUP_TEXT_OPTION),\
        help    = STRING_TOKEN(STR_SETUP_TEXT_OPTION_HELP),\
        option text = STRING_TOKEN(STR_INACTIVE),  value = 0, flags = RESET_REQUIRED;\
        option text = STRING_TOKEN(STR_ACTIVE),  value = 1, flags = DEFAULT | MANUFACTURING | RESET_REQUIRED;\
        endoneof;           
#endif //#ifdef CONTROL_DEFINITION

#ifdef ADVANCED_FORM_SET
 
  #ifdef FORM_SET_GOTO
    // Define goto commands for the forms defined in this file
    goto PCI_NETWORK_FORM_ID, 
      prompt = STRING_TOKEN(STR_SETUP_TEXT_OPTION),
      help = STRING_TOKEN(STR_SETUP_TEXT_OPTION_HELP),
      flags   = INTERACTIVE, 
      key     = AUTO_ID(SHOW_SETUP_TEXT_FORM_KEY);
      
  #endif
  #ifdef FORM_SET_FORM
  // Define forms
    form formid = AUTO_ID(PCI_NETWORK_FORM_ID),
    title = STRING_TOKEN(STR_SETUP_TEXT_OPTION_HELP);
    SUBTITLE(STRING_TOKEN(STR_DISPLAY_PCI))
    SUBTITLE(STRING_TOKEN(STR_SEPERATE_LINE))
    SHOWPCINETWORK
    suppressif  ideqval  SETUP_DATA.PcideviceDisplay == 0x0;
    text
    help   = STRING_TOKEN(STR_DISPLAY_PCI),
    text   = STRING_TOKEN(STR_DISPLAY_PCI),
    text   = STRING_TOKEN(STR_PCI_DEVICE_VALUE),
    flags  = 0,
    key    =0;
    endif;
   // suppressif ideqval SETUP_DATA.pcishowSupport == 0;
   // SHOWPCINETWORK
   
    text
    help   = STRING_TOKEN(STR_DISPLAY_SMBIOS0),
    text   = STRING_TOKEN(STR_DISPLAY_SMBIOS0),
    text   = STRING_TOKEN(STR_SMBIOS_VALUE0),
    flags  = 0,
    key    =0;
  
  text
    help   = STRING_TOKEN(STR_DISPLAY_SMBIOS1),
    text   = STRING_TOKEN(STR_DISPLAY_SMBIOS1),
    text   = STRING_TOKEN(STR_SMBIOS_VALUE1),
    flags  = 0,
    key    =0;
    endform;
 
  #endif//FORM_SET_FORM
 
