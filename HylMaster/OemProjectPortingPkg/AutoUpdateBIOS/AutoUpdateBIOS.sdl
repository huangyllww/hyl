TOKEN
	Name  = AutoUpdateBIOS_SUPPORT
	Value  = "0"
	TokenType = Boolean
	TargetMAK = Yes
	Master = Yes
	Help  = "Main switch to enable ROM support in Project"
End

PATH
	Name  = "AutoUpdateBIOS_DIR"
End

MODULE
  File  = "AutoUpdateBIOS.mak"
End

TOKEN
	Name  = "SISTool"
	Value  = "$(AutoUpdateBIOS_DIR)\sistool.exe "
	TokenType = Expression
	TargetMAK = Yes
End

TOKEN
	Name  = "IPMITool"
	Value  = "$(AutoUpdateBIOS_DIR)\ipmitool.exe "
	TokenType = Expression
	TargetMAK = Yes
End


TOKEN
	Name  = "HostIP"
	Value  = "10.8.80.15"
	TokenType = Expression
	TargetMAK = Yes
End

TOKEN
	Name  = "UserName"
	Value  = "admin"
	TokenType = Expression
	TargetMAK = Yes
End

TOKEN
	Name  = "UserPwd"
	Value  = "admin"
	TokenType = Expression
	TargetMAK = Yes
End