PCIDEVICE
    Title  = "Root Complex Socket0 Die0"
    Parent  = "PciHost (Virtual)"
#    Attribute  = "0x7307f"   - this sets VGA bits. Try without
    Attribute  = "0x303f"
    Dev_type  = "RootBridge"
    Bus  = 00h
    Dev  = 00h
    Fun  = 00h
    BridgeBusNum  = 00h
    SleepNum  = 01h
    ASLfile  = "'AmiChipsetModulePkg/AslSL1r2/Nb.ASL','AmiChipsetModulePkg/AslSL1r2/Rb_Res.asl','AmiModulePkg\PCI\OSCM.ASL'"
    ASLdeviceName  = "PCI0"
    DeviceType = OnBoard
    PCIBusSize = 32bit
    ROMMain = No
    FixedBus = Yes
    PCIExpress = Yes
    ASL_PTS  = "Method;\_SB.PCI0.NPTS(Arg0)"
    ASL_WAK  = "Method;ShiftLeft(Arg0, 4, DBG8)  \_SB.PCI0.NWAK(Arg0)"
    InitRoutine  = "RootBrgInit"
End

