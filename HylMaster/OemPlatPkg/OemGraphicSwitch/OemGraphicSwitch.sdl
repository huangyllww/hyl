OUTPUTREGISTER
	Name  = "OemGraphicSwitchElink"
	Path  = "Build"
	File  = "OemGraphicSwitchElink.h"
	Template  = "OemGraphicSwitchElink.txt"
End

INFComponent
	Name  = "OemGraphicSwitch"
