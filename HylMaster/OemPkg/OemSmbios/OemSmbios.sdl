	TokenType = Boolean
	TargetEQU = Yes
	TargetMAK = Yes
	Master = Yes
	#Token = "AUTO_IIO_BIFURCATION_SUPPORT" "=" "1"
End

PATH
	Name  = "SUGOMSMBIOS_DIR"
End

INFComponent
  Name  = "OemSmbios"
