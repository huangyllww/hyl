TOKEN
	Name  = FCIV_SUPPORT
	Value  = "1"
	TokenType = Boolean
	TargetMAK = Yes
	Master = Yes
	Help  = "Main switch to enable ROM support in Project"
End

TOKEN
	Name  = "FCIV"
	Value  = "$(FCIV_DIR)\fciv.exe "
	TokenType = Expression
	TargetMAK = Yes
	Help = "Add switch -@ for Silent operation"
End

PATH
	Name  = "FCIV_DIR"
End

MODULE
      File  = "FCIV.mak"
End
