TOKEN
	Name  = "HardwareMonitor_SUPPORT"
	Value  = "1"
	Help  = "Main switch to enable HardwareMonitor support in Project"
	TokenType = Boolean
	TargetMAK = Yes
	Master = Yes
	Token = "IPMI_SUPPORT" "=" "1"
End

MODULE
	Help  = "Includes \HardwareMonitor.mak to Project"
	File  = "HardwareMonitor.mak"
End

PATH
	Name  = "Hardware_Monitor_DIR"
	Path  = "OemPkg/OemHardwareMonitor"
End

#ELINK
#	Name  = "$(Hardware_Monitor_DIR)/HardwareMonitor.sd"
#	Parent  = "SETUP_DEFINITIONS"
#	InvokeOrder = AfterParent
#End

ELINK
	Name  = "$(BUILD_DIR)/SensorEntry.sd"
	Parent  = "SETUP_DEFINITIONS"
	InvokeOrder = AfterParent
End

ELINK
	Name  = "$(Hardware_Monitor_DIR)/HardwareMonitor.uni"
	Parent  = "SetupStringFiles"
	InvokeOrder = AfterParent
End

ELINK
	Name  = "$(BUILD_DIR)/SensorEntryStr.uni"
	Parent  = "SetupStringFiles"
	InvokeOrder = AfterParent
End

ELINK
	Name  = "$(Hardware_Monitor_DIR)/HardwareMonitor.c"
	Parent  = "SetupCallbackFiles"
	InvokeOrder = AfterParent
End

TOKEN
	Name  = "SENSOR_ENTRY_NUMBER"
	Value  = "200"
	Help  = "The Account of Sensor Entry will be generated"
	TokenType = Integer
	TargetMAK = Yes
	TargetH	= Yes
End

ELINK
	Name  = "ITEM_CALLBACK(ADVANCED_FORM_SET_CLASS,0,VIEW_HW_MONITOR_FORM_KEY, InitSensorCallbackFunction),"
	Parent  = "SetupItemCallbacks"
	InvokeOrder = AfterParent
End

ELINK
	Name  = "ITEM_CALLBACK(ADVANCED_FORM_SET_CLASS,0,SENSOR_VALUE_KEY,HWMonitorCallbackFunction),"
	Parent  = "SetupItemCallbacks"
	InvokeOrder = AfterParent
End

TOKEN
	Name  = "SERVER_IPMI_DEBUG_MESSAGE_SUPPORT"
	Value  = "1"
	Help  = "Enables/Disables Debug message support from AMI IPMI PKG. Default is disabled. This token is provided to track flow of the modules when IPMI module is not working as expected. "
	TokenType = Boolean
	TargetH = Yes
	Token = "DEBUG_MODE" "=" "1"
End
