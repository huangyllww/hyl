    Name  = "COMBO_HDR_ID_SOC0"
    Value  = "0x0"
    Help  = "ID for this SOC, Not supported"
    TokenType = Integer
    TargetMAK = Yes
    TargetH = Yes
End

TOKEN
    Name  = "COMBO_HDR_ID_SOC0"
    Value  = "0xBC090201"
    Help  = "ID for this SOC, Not supported"
    TokenType = Integer
    TargetMAK = Yes
    TargetH = Yes
    Token = "MULTI_CPU_SUPPORT" "=" "1"
End

ELINK
    Name  = '<COMBO_ENTRY IdSelect="0x00" Id="$(COMBO_HDR_ID_SOC0)" Address="$(PSPDIR_L1_BASE_SOC0)"/>'
    Parent  = "COMBO_ENTRY_ELINK"
    InvokeOrder = AfterParent
    Help = "Add this PSP level one into PSP Direcotry for Multi CPU support."
    Token = "MULTI_CPU_SUPPORT" "=" "1"
End

