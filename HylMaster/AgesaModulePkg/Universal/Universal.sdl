	Name  = "AmdAcpiDxe_SUPPORT"
	Value  = "1"
	Help  = "Main switch to enable AmdAcpiDxe support in Project"
	TokenType = Boolean
End

TOKEN
	Name  = "AmdMemoryHobInfoPeim_SUPPORT"
	Value  = "1"
	Help  = "Main switch to enable AmdMemoryHobInfoPeim support in Project"
