	INCLUDE token.equ

.686p
.model  flat
.data
	dd '_BCP'
_BCPFlashDataSize label dword
	dd offset BCPFlashDataEnd - offset BCPFlashDataStart
;align 4
BCPFlashDataStart EQU $
        db MKF_FLASHDATA_SIZE_BCP dup (0FFh)
BCPFlashDataEnd label byte
	db 4 dup (0FFh)	;Mark end of table. Same size as each flash data entry.
end

