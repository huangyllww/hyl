    Template  = "NVMeTopyPorting.c"
End

ELINK
	Name  = "gNVMeConfigList_DualCPUInstalled,"
	Parent  = "DUALL_CPU_INSTALLED_CONFIG,"
	InvokeOrder = ReplaceParent
End

ELINK
	Name  = "gNVMeConfigList_SingleCPUInstalled,"
	Parent  = "SINGLE_CPU_INSTALLED_CONFIG,"
	InvokeOrder = ReplaceParent
End

