TOKEN
    Name  = "SATA_DEVICE_INFO_SUPPORT"
    Value  = "1"
    Help  = "Main switch to enable the Sata Controller and device information display in the Setup"
    TokenType = Boolean
    TargetH = Yes
    Master = Yes
End

TOKEN
    Name  = "DISPLAY_HDD_FEATURE_INFO"
    Value  = "0"
    Help  = "Displays additional Hdd informations in separate sub menu"
    TokenType = Boolean
    TargetH = Yes
End

ELINK
    Name  = "SataContollerNameUpdatelist"
    InvokeOrder = ReplaceParent
    Help  = "Contains the Controller information list in the format of( {Vendor ID, Device ID, Controller Name} }, Vendor,Device ID and OEM defined name of the controller has to be given"
End

OUTPUTREGISTER
    Name = "SataContollerNameElinkFile"
    File = "SataControllerNameELink.h"
    Path = "Build"
    Template = "SataDevInfoElink.txt"
End

ELINK
    Name = "{ { 0x5d9265f7, 0xe3ec, 0x4be1, { 0xa9, 0x95, 0x85, 0xd8, 0x60, 0xa5, 0xa4, 0x2e } }, 0, NULL},"
    Parent  = "AMITSE_HIDDEN_PAGE_LIST"
    Help  = "Provide GUIDs of dynamic formset to suppress. Terminate entry with comma"
    InvokeOrder = AfterParent
End

PATH
    Name  = "SATA_DEVICE_INFO_DIR"
End

ELINK
    Name = "$(SATA_DEVICE_INFO_DIR)/SataDevInfo.sd"
    Parent = "SETUP_DEFINITIONS"
    InvokeOrder = AfterParent
End

ELINK
    Name  = "$(SATA_DEVICE_INFO_DIR)/SataDevInfo.uni"
    Parent  = "SetupStringFiles"
    InvokeOrder = AfterParent
End

INFComponent
    Name  = "SataDevInfo"
    File  = "SataDevInfo.inf"
    Package  = "AmiModulePkg"
    ModuleTypes  = "DXE_DRIVER"
End
