################################################################################
#
#@Description
#   Boot Option Priorities
#
################################################################################
TOKEN
  Name  = "SEPARATED_UEFI_DEV"
  Value  = "1"
  Help  = "Control token BOOT_OPTION_TAG_PRIORITIES"
  TokenType = Boolean
  TargetH = Yes
End

TOKEN
  Name  = "BOOT_OPTION_TAG_PRIORITIES"
  Value  = "BoTagUefiUsbKey, BoTagUefiUsbCdrom, BoTagUefiUsbHardDisk, BoTagUefiHardDisk, BoTagUefiIP4NetWork, BoTagUefiIP6NetWork, BoTagUefiNetWork, BoTagUefiIP4UriNetWork, BoTagUefiIP6UriNetWork, BoTagUefiUriNetWork, BoTagEmbeddedShell, BoTagUefiCdrom, BoTagLegacyUsb, BoTagLegacyHardDisk, BoTagLegacyBevDevice, BoTagLegacyEmbedNetwork, BoTagLegacyCdrom, BoTagLegacyFloppy"
  Help  = "List of value of type BOOT_OPTION_TAG that define priorities of the boot option tags.\BOOT_OPTION_TAG is defined in BdsBoard.c"
  TokenType = Expression
  TargetH = Yes
  Token = "SEPARATED_UEFI_DEV" "=" "1"
End

TOKEN
  Name  = "BOOT_OPTION_TAG_PRIORITIES"
  Value  = "BoTagLegacyUsb, BoTagLegacyHardDisk, BoTagLegacyEmbedNetwork, BoTagEmbeddedShell, BoTagUefi, BoTagLegacyCdrom, BoTagLegacyFloppy, BoTagLegacyBevDevice"
  Help  = "List of value of type BOOT_OPTION_TAG that define priorities of the boot option tags.\BOOT_OPTION_TAG is defined in BdsBoard.c"
  TokenType = Expression
  TargetH = Yes
  Token = "SEPARATED_UEFI_DEV" "=" "0"
End

TOKEN
  Name  = "BOOT_OPTION_DEVICE_TAG_PRIORITIES"
