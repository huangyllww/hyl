#ifdef FORM_SET_ITEM

#define REAR_FRONT_HUB30 \
    oneof varid = SETUP_DATA.RearFrontHub30,\
      prompt      = STRING_TOKEN (STR_REAR_FRONT_HUB30_PROMPT),\
      help        = STRING_TOKEN (STR_REAR_FRONT_HUB30_PROMPT_HELP),\
      option text = STRING_TOKEN (STR_XHCI_PORT_ENABLED),        value = 1,    flags = DEFAULT | MANUFACTURING | RESET_REQUIRED;\
      option text = STRING_TOKEN (STR_XHCI_PORT_DISABLED),         value = 0,    flags = 0 | RESET_REQUIRED;\
    endoneof;
    
#define USB3_TYPE_A \
    oneof varid = SETUP_DATA.Usb3TypeAPort,\
      prompt      = STRING_TOKEN (STR_USB3_TYPE_A_PROMPT),\
      help        = STRING_TOKEN (STR_USB3_TYPE_A_PROMPT_HELP),\
      option text = STRING_TOKEN (STR_XHCI_PORT_ENABLED),        value = 1,    flags = DEFAULT | MANUFACTURING | RESET_REQUIRED;\
      option text = STRING_TOKEN (STR_XHCI_PORT_DISABLED),         value = 0,    flags = 0 | RESET_REQUIRED;\
    endoneof;
    
#define BMC_USB_PORT \
    oneof varid = SETUP_DATA.BmcUsbPort,\
      prompt      = STRING_TOKEN (STR_BMC_USB_PORT_PROMPT),\
      help        = STRING_TOKEN (STR_BMC_USB_PORT_PROMPT_HELP),\
      option text = STRING_TOKEN (STR_XHCI_PORT_ENABLED),        value = 1,    flags = DEFAULT | MANUFACTURING | RESET_REQUIRED;\
      option text = STRING_TOKEN (STR_XHCI_PORT_DISABLED),         value = 0,    flags = 0 | RESET_REQUIRED;\
    endoneof;
    
#define OCP_USB20 \
    oneof varid = SETUP_DATA.OCPUsb20,\
      prompt      = STRING_TOKEN (STR_OCP_USB20_PROMPT),\
      help        = STRING_TOKEN (STR_OCP_USB20_PROMPT_HELP),\
      option text = STRING_TOKEN (STR_XHCI_PORT_ENABLED),        value = 1,    flags = DEFAULT | MANUFACTURING | RESET_REQUIRED;\
      option text = STRING_TOKEN (STR_XHCI_PORT_DISABLED),         value = 0,    flags = 0 | RESET_REQUIRED;\
    endoneof;
    
#endif

#ifdef FORM_SET_GOTO
    suppressif NOT ideqval OEM_NO_DEFAULT_VARIABLES.OemHide == 0x11;
        goto   XHCI_PORT_ID,
        prompt = STRING_TOKEN (STR_XHCI_PORT_FORM),
        help   = STRING_TOKEN (STR_XHCI_PORT_HELP);
    endif;
#endif

#ifdef FORM_SET_FORM
    form formid = AUTO_ID(XHCI_PORT_ID),
        title = STRING_TOKEN(STR_XHCI_PORT_FORM);
  
      //  USB3_TYPE_A
        REAR_FRONT_HUB30
        USB3_TYPE_A
        BMC_USB_PORT
        OCP_USB20
 
            endform;
#endif

