
[Defines]
  INF_VERSION                    = 0x00010005
  BASE_NAME                      = SbGenericRuntime
  MODULE_TYPE                    = DXE_DRIVER
  VERSION_STRING                 = 1.0
  LIBRARY_CLASS                  = SBGeneric|DXE_RUNTIME_DRIVER
  CONSTRUCTOR                    = SBGenericRuntimeLibConstructor

[Packages]
  MdePkg/MdePkg.dec 
  IntelFrameworkPkg/IntelFrameworkPkg.dec
  AmiCompatibilityPkg/AmiCompatibilityPkg.dec 
  AmiModulePkg/AmiModulePkg.dec
  AgesaPkg/AgesaPkg.dec
  AmiChipsetPkg/AmiChipsetPkg.dec  
  AmiChipsetModulePkg/AmiChipsetModulePkg.dec

[LibraryClasses]
  BaseLib
  AmiChipsetIoLib
  AmiIoDecodeLib


[Sources]
  SbGeneric.c

[BuildOptions]
  MSFT:*_*_*_CC_FLAGS = /D SB_GENERIC_RUNTIME
  GCC:*_*_*_CC_FLAGS = -D SB_GENERIC_RUNTIME 
  
[Pcd]
  gEfiMdePkgTokenSpaceGuid.PcdPciExpressBaseAddress
  gAmiChipsetPkgTokenSpaceGuid.PcdAcpiIoPortBaseAddress
  gAmiChipsetModulePkgTokenSpaceGuid.PcdGpioPortBaseAddress
  gAmiChipsetModulePkgTokenSpaceGuid.PcdSbWakeType