	ModuleTypes  = "DXE_DRIVER"
	Token = "Pca9555aDxe_SUPPORT" "=" "1"
End

INFComponent
	Name  = "Pca9555aPei"
	File  = "Pei/Pca9555aPei.inf"
	Package  = "AmdCpmPkg"
	ModuleTypes  = "PEIM"
	Token = "Pca9555aPei_SUPPORT" "=" "1"
End

