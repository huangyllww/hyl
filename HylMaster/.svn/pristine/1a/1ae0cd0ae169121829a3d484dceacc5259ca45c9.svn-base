                prompt  = STRING_TOKEN(STR_MASS_STORAGE_OPROM_POLICY_EX),\
                help    = STRING_TOKEN(STR_MASS_STORAGE_OPROM_POLICY_HELP),\
                default = DEFAULT_MASS_STORAGE_OPROM_POLICY_EX,\
