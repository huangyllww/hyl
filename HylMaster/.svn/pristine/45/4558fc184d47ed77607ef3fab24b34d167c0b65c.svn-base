PCIDEVICE
    Title  = "Root Complex Socket1 Die1"
    Parent  = "PciHost (Virtual)"
#    Attribute  = "0x7307f"   - this sets VGA bits. Try without
    Attribute  = "0x303f"
    Dev_type  = "RootBridge"
    Bus  = 00h
    Dev  = 00h
    Fun  = 00h
    BridgeBusNum  = 05h
    SleepNum  = 01h
