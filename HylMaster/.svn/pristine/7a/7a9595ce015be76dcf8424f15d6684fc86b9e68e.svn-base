<?xml version="1.0" ?>
<DIRS AddressMode="0">
    <!-- Tag: "PSP_DIR"  Specify the entries belong to PSP Directory -->
    <!-- Attribute SIZE: -->
    <!-- If "Size" attribute has been declared, it will be used to specify the whole size reserve for PSP entries, tool will automatically calculate the address for child "IMAGE_ENTRY"-->

    <!-- Tag: "BIOS_DIR"  Specify the entries belong to BIOS Directory, the difference between BIOS and PSP directory,is the "Destination" filed only valid for BIOS directory -->
    <!-- Attribute SIZE: -->
    <!-- If "Size" attribute has been declared, it will be used to specify the whole size reserve for PSP entries, tool will automatically calculate the address for child "IMAGE_ENTRY"-->
    <!-- If "Size" attribute has NOT been declared explicitly, the "address" should be declared in all child "IMAGE_ENTRY"s -->
    <!--     SpiBlockSize speicify the SPI block size, tool will align the modifiable entries with SpiBlockSize, other entries will aligned on 0x100 boundary -->
    <!-- PSP DIR Fixed region, should be protected during BIOS updated -->
  <PSP_DIR Level="1" Base="0x00120000" Size="0x80000" SpiBlockSize="0x1000">
    <!-- Recovery purpose FWs, will be place in the Fix region -->
    <!-- AMD Public Key -->
    <IMAGE_ENTRY Type="0x00" File="AmdPubKeyZP.bin"/>
    <!-- Point to PSP Boot Loader firmware in 2nd level DIR, This entry is used to satisfie PSP bootrom -->
    <POINT_ENTRY Type="0x01" Address="0x001D0400" Size="0x20000"/>
    <!-- PSP recovery Boot Loader firmware -->
    <IMAGE_ENTRY Type="0x03" File="PspRecoveryBootLoader_prod_ZPSp3Sp4.sbin"/>
    <!-- SMU offchip firmware -->
    <IMAGE_ENTRY Type="0x08" File="SmuFirmwareMcmZP.csbin"/>
    <!-- Signed OEM ABL Key -->
    <IMAGE_ENTRY Type="0x0A" File="ZeppelinOemAblFw.tkn"/>
    <!-- SMU offchip firmware 2-->
    <!-- This entry is required if recovery path need proceed to DXE phase -->
    <!-- Only support by PSP FW with "PLAT-16852 â€“ PSP recovery boot loader to load SMU2 firmware to DRAM if available." -->
    <!-- <IMAGE_ENTRY Type="0x12" File="SmuFirmware2ZP.csbin"/> -->
    <!-- Wrapped iKEK -->
    <IMAGE_ENTRY Type="0x21" File="PspIkekZP.bin"/>
    <!-- PSP AGESA Bootloaders -->
    <!-- Entry to load security gasket binary -->
    <IMAGE_ENTRY Type="0x24" File="RsmuSecurityPolicy_ZP.sbin"/>
    <IMAGE_ENTRY Type="0x30" File="AgesaBootloader0_prod_Mcm_ZP.csbin"/>
    <IMAGE_ENTRY Type="0x31" File="AgesaBootloader1_prod_Mcm_ZP.csbin"/>
    <IMAGE_ENTRY Type="0x32" File="AgesaBootloader2_prod_Mcm_ZP.csbin"/>
    <IMAGE_ENTRY Type="0x33" File="AgesaBootloader3_prod_Mcm_ZP.csbin"/>
    <IMAGE_ENTRY Type="0x34" File="AgesaBootloader4_prod_Mcm_ZP.csbin"/>
    <IMAGE_ENTRY Type="0x35" File="AgesaBootloader5_prod_Mcm_ZP.csbin"/>
    <IMAGE_ENTRY Type="0x36" File="AgesaBootloader6_prod_Mcm_ZP.csbin"/>
    <!-- Point to 2nd level PSP Hdr -->
    <POINT_ENTRY Type="0x40" Address="0x001D0000" Size="0x400"/>
  </PSP_DIR>
  <!-- BIOS DIR Fixed region, should be protected during BIOS updated -->
  <BIOS_DIR Level="1" Base="0x001A0000" Size="0x30000" SpiBlockSize="0x1000">
    <!-- Refer to BIOS guide for detail requirement of each Type -->
    <!-- All definition is same as PSP_DIR except Destination -->
    <!-- Attribute Destination:(Optional)-->
    <!-- Destination: Used to specify the destination memory address PSP FW need copy to -->
    <!-- Recovery purpose FWs, will be place in the Fix region -->
    <IMAGE_ENTRY Type="0x05" File="RtmPubSignedZP.key"/>
    <!-- The RTMSignature.bin is NOT ready when generate Header with "BD" command, Use explicit "Size" to avoid open file fail  -->
    <IMAGE_ENTRY Type="0x07" File="RTMSignature.bin" Size="0x100"/>
    <!-- Agesa PSP Customization Block (PSP Agesa variable) NVRAM (APCB)-->
    <IMAGE_ENTRY Type="0x60" File="APCB_ZP_D4.bin" Size="0x4000"/>

    <!-- Agesa PSP Customization Block (PSP Agesa variable) NVRAM (APCB) BackUp Copy-->
    <IMAGE_ENTRY Type="0x68" Instance="0x00" File="APCB_ZP_D4.bin" Size="0x4000"/>
    <!-- APOB will be filled @0x04000000 which specify by the Destination -->
    <POINT_ENTRY Type="0x61" Address="0" Size="0" Destination="0x04000000"/>
    <!-- FV_RECOVERY will copy to Temp Memory 0x01000000 as specified by the Destination, also will
        authenticate due the ResetImage Flag set to 1 -->
    <POINT_ENTRY Address="0xE00000L" Destination="0x01E00000" Size="0x200000" Type="0x62">
      <TypeAttrib Copy="0x1" ReadOnly="0x0" RegionType="0x0" ResetImage="0x1"/>
    </POINT_ENTRY>
    <!-- APPB PMU Firmware -->
    <IMAGE_ENTRY Type="0x64" Instance="0x01" File="Appb_Zp_1D_Ddr4_Udimm_Imem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x65" Instance="0x01" File="Appb_Zp_1D_Ddr4_Udimm_Dmem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x64" Instance="0x02" File="Appb_Zp_1D_Ddr4_Rdimm_Imem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x65" Instance="0x02" File="Appb_Zp_1D_Ddr4_Rdimm_Dmem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x64" Instance="0x03" File="Appb_Zp_1D_Ddr4_Lrdimm_Imem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x65" Instance="0x03" File="Appb_Zp_1D_Ddr4_Lrdimm_Dmem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x64" Instance="0x04" File="Appb_Zp_2D_Ddr4_Imem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x65" Instance="0x04" File="Appb_Zp_2D_Ddr4_Dmem_Mcm.csbin" />
    <!-- Point to 2nd level BIOS DIR Hdr -->
    <POINT_ENTRY Type="0x70" Address="0x002F0000" Size="0x400"/>
  </BIOS_DIR>
  <!-- PSP Level 2 DIR -->
  <!-- Attribute Level: default value 1 if not specified -->
  <!-- 1: Indentify 1st level PSP DIR, it used as fixed region in recovery case -->
  <!-- 2: Indentify 2nd level PSP DIR, it used as updatable region in recovery case -->
  <!-- PSP DIR updatable region, can be updated during BIOS updated, any entry corruption will trigger recovery process -->
  <PSP_DIR Level="2" Base="0x001D0000" Size="0x120000" SpiBlockSize="0x1000">
    <!-- Updateable FWs, will be place in the Updatable region -->
    <IMAGE_ENTRY Type="0x01" File="PspBootLoader_prod_SmAm4.sbin" Size="0x20000"/>
    <!-- PSP Boot Loader firmware -->
    <IMAGE_ENTRY Type="0x01" File="PspBootLoader_prod_ZpSp3Sp4.sbin"/>
    <!-- SMU offchip firmware -->
    <IMAGE_ENTRY Type="0x08" File="SmuFirmwareMcmZP.csbin"/>
    <!-- AMD Secure Debug Key -->
    <IMAGE_ENTRY Type="0x09" File="ZeppelinSecureDebug.tkn"/>
    <!-- Signed OEM ABL Key -->
    <IMAGE_ENTRY Type="0x0A" File="ZeppelinOemAblFw.tkn"/>

    <VALUE_ENTRY Type="0x0B" Value="0x1"/>
    <!-- SMU offchip firmware 2-->
    <IMAGE_ENTRY Type="0x12" File="SmuFirmware2McmZP.csbin"/>
    <!-- PSP early secure unlock debug image -->
    <IMAGE_ENTRY Type="0x13" File="DebugUnlock_ZP.sbin"/>
    <!-- Wrapped iKEK -->
    <IMAGE_ENTRY Type="0x21" File="PspIkekZP.bin"/>
    <!-- Token unlock Data -->
    <IMAGE_ENTRY Type="0x22" File="SecureEmptyToken.bin"/>
    <!-- Entry to load security gasket binary -->
    <IMAGE_ENTRY Type="0x24" File="RsmuSecurityPolicy_ZP.sbin"/>
    <!-- PSP AGESA Bootloaders -->
    <IMAGE_ENTRY Type="0x30" File="AgesaBootloader0_prod_Mcm_ZP.csbin"/>
    <IMAGE_ENTRY Type="0x31" File="AgesaBootloader1_prod_Mcm_ZP.csbin"/>
    <IMAGE_ENTRY Type="0x32" File="AgesaBootloader2_prod_Mcm_ZP.csbin"/>
    <IMAGE_ENTRY Type="0x33" File="AgesaBootloader3_prod_Mcm_ZP.csbin"/>
    <IMAGE_ENTRY Type="0x34" File="AgesaBootloader4_prod_Mcm_ZP.csbin"/>
    <IMAGE_ENTRY Type="0x35" File="AgesaBootloader5_prod_Mcm_ZP.csbin"/>
    <IMAGE_ENTRY Type="0x36" File="AgesaBootloader6_prod_Mcm_ZP.csbin"/>
    <IMAGE_ENTRY Type="0x38" File="PspSevEmptyData.bin"/>
    <IMAGE_ENTRY Type="0x39" File="PspSevUapp.sbin"/>
  </PSP_DIR>

    <!-- Refer to BIOS guide for detail requirement of each Type -->
    <!-- All definition is same as PSP_DIR except Destination -->
    <!-- Attribute Destination:(Optional)-->
    <!-- Destination: Used to specify the destination memory address PSP FW need copy to -->
    <!-- BIOS DIR updatable region, can be updated during BIOS updated, any entry corruption will trigger recovery process -->
  <BIOS_DIR Level="2" Base="0x002F0000" Size="0x30000" SpiBlockSize="0x1000">
    <!-- Refer to BIOS guide for detail requirement of each Type -->
    <!-- All definition is same as PSP_DIR except Destination -->
    <!-- Attribute Destination:(Optional)-->
    <!-- Destination: Used to specify the destination memory address PSP FW need copy to -->
    <IMAGE_ENTRY Type="0x05" File="RtmPubSignedZP.key"/>
    <!-- The RTMSignature.bin is NOT ready when generate Header with "BD" command, Use explicit "Size" to avoid open file fail  -->
    <!-- <IMAGE_ENTRY Type="0x07" File="RTMSignature.bin" Size="0x100"/> -->

    <!-- Agesa PSP Customization Block (PSP Agesa variable) NVRAM (APCB)-->
    <IMAGE_ENTRY Type="0x60" File="APCB_ZP_D4.bin" Size="0x4000"/>

    <!-- Agesa PSP Customization Block (PSP Agesa variable) NVRAM (APCB) BackUp Copy-->
    <IMAGE_ENTRY Type="0x68" Instance="0x00" File="APCB_ZP_D4.bin" Size="0x4000"/>
    <!-- APOB will be filled @0x04000000 which specify by the Destination -->
    <POINT_ENTRY Type="0x61" Address="0" Size="0" Destination="0x04000000"/>
    <!-- The RTMSignature.bin is NOT ready when generate Header with "BD" command, Use explicit "Size" to avoid open file fail  -->
    <IMAGE_ENTRY Type="0x07" File="RTMSignatureL1L2.bin" Size="0x100"/>
    <!-- FV_RECOVERY will copy to Temp Memory 0x01000000 as specified by the Destination, also will
        authenticate due the ResetImage Flag set to 1 -->
    <POINT_ENTRY Address="0xE00000L" Destination="0x01E00000" Size="0x200000" Type="0x62">
      <TypeAttrib Copy="0x1" ReadOnly="0x0" RegionType="0x0" ResetImage="0x1"/>
    </POINT_ENTRY>

    <!-- APOB NVRAM COPY-->
    <IMAGE_ENTRY Type="0x63" File="APOB_NV_ZP.bin" />

    <!-- MICROCODE PATCH -->
    <!-- Please note the Instance of MICROCODE PATCH should be continual-->
    <IMAGE_ENTRY Type="0x66" Instance="0x00" File="UcodePatch_ZP_B2.bin" />
    <IMAGE_ENTRY Type="0x66" Instance="0x01" File="UcodePatch_ZP_B1.bin" />
    <IMAGE_ENTRY Type="0x66" Instance="0x02" File="UcodePatch_ZP_B0.bin" />
    <IMAGE_ENTRY Type="0x66" Instance="0x03" File="UcodePatch_ZP_A0.bin" />

    <!-- APPB PMU Firmware -->
    <IMAGE_ENTRY Type="0x64" Instance="0x01" File="Appb_Zp_1D_Ddr4_Udimm_Imem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x65" Instance="0x01" File="Appb_Zp_1D_Ddr4_Udimm_Dmem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x64" Instance="0x02" File="Appb_Zp_1D_Ddr4_Rdimm_Imem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x65" Instance="0x02" File="Appb_Zp_1D_Ddr4_Rdimm_Dmem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x64" Instance="0x03" File="Appb_Zp_1D_Ddr4_Lrdimm_Imem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x65" Instance="0x03" File="Appb_Zp_1D_Ddr4_Lrdimm_Dmem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x64" Instance="0x04" File="Appb_Zp_2D_Ddr4_Imem_Mcm.csbin" />
    <IMAGE_ENTRY Type="0x65" Instance="0x04" File="Appb_Zp_2D_Ddr4_Dmem_Mcm.csbin" />

    <!-- Core Machine Check Exception Data -->
    <IMAGE_ENTRY Type="0x67" Instance="0x00" File="CoreMceEmptyData.bin" />
  </BIOS_DIR>
</DIRS>