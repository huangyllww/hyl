    ModuleTypes  = "USER_DEFINED"
End

FFS_FILE
    Name  = "AmiBoardInfoDataZp"
    FD_AREA  = "FV_MAIN"
    INFComponent  = "AmiModulePkg.BrdInfoOutputZp"
    Rule_Override  = "SubtypeGuidCompressed"
End



