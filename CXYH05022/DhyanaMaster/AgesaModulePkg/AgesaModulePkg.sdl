TOKEN
	Name  = "AgesaModulePkg_SUPPORT"
	Value  = "1"
	Help  = "Switch for Enabling AgesaModulePkg support in the project"
	TokenType = Boolean
	Master = Yes
	Disable = Yes
End

TOKEN
	Name  = "Build_AgesaModulePkg"
	Value  = "0"
	Help  = "Switch for building AgesaModulePkg Package"
	TokenType = Boolean
End

TOKEN
	Name  = "PLATFORM_NAME"
	Value  = "AgesaModulePkg"
	TokenType = Expression
	TargetDSC = Yes
	Token = "Build_AgesaModulePkg" "=" "1"
End

