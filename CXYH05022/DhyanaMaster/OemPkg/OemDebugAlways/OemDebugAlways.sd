#ifdef FORM_SET_VARSTORE

    varstore OEM_COMMON_SETUP_DATA,
      key   = OEM_COMMON_VAR_KEY,
      name  = OEM_VAR,
      guid  = OEM_COMMON_VAR_GUID;

