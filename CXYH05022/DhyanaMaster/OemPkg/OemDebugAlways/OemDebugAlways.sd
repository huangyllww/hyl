#ifdef FORM_SET_GOTO
	// Define goto commands for the forms defined in this file
    suppressif NOT ideqval OEM_NO_DEFAULT_VARIABLES.OemHide == 0x11;  
	goto OEM_DEBUG_MODE_ID, 
	prompt = STRING_TOKEN(STR_DEBUG_MODE_SETTING),
	help = STRING_TOKEN(STR_DEBUG_MODE_HELP);	
	endif;
#endif

#ifdef FORM_SET_FORM

	form formid = AUTO_ID(OEM_DEBUG_MODE_ID),
		title = STRING_TOKEN(STR_DEBUG_MODE_SETTING);

		SEPARATOR

		DEBUG_MODE_SETTING
        
        //Mizl-20220222-SGPZT#22027 Add Debug Level Lib Support+>
        suppressif NOT ideqval OEM_COMMON_SETUP_DATA.DebugModeMask == 1;
        goto
            formsetguid = OEM_DEBUG_LEVEL_FORM_SET_GUID,
            formid = 1,
            question = 0,
            prompt = STRING_TOKEN(STR_OEM_DEBUG_LEVEL_GOTO_FORM),
            help = STRING_TOKEN(STR_OEM_DEBUG_LEVEL_GOTO_FORM_HELP);
        endif;
        //Mizl-20220222-SGPZT#22027 Add Debug Level Lib Support->

        //<licm-20210226 VGA and UART Display 16bit Port80 SUPPORT +>
        #if defined(VGA_UART_DIS16BIT_PORT80_SUPPORT) && (VGA_UART_DIS16BIT_PORT80_SUPPORT == 1)
        SEPARATOR  //Mizl-20220222-SGPZT#22027 Add Debug Level Lib Support>
        POST_EX_SUPPORT
        #endif
        //<licm-20210226 VGA and UART Display 16bit Port80 SUPPORT ->

	endform;
#endif 

#endif//ADVANCED_FORM_SET