TOKEN
	Name  = "OemProjectPortingPkg_SUPPORT"
	Value  = "1"
	Help  = "Main switch to enable OemPlatPkg support in Project"
	TokenType = Boolean
	TargetEQU = Yes
	TargetMAK = Yes
	Master = Yes
End

PATH
    Name  = "OEM_PROJECT_PORTING_PKG_DIR"
End

TOKEN
    Name  = "OemProduction_BIOS"
    Value  = "1"
    Help  = "0 - None production BIOS, include debug message and function.\1 - Production BIOS, remove debug message."
    TokenType = Boolean
    TargetH = Yes
End
