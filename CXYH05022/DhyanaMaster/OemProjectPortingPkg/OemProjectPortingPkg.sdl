#
############################################################################
TOKEN
	Name  = "MIN_Index_NOT_SENT_TO_BMC"
	Value  = "7"
	Help  = "Fast Video output Text Content"
	TokenType = Integer
	TargetMAK = Yes
	TargetH = Yes
End

TOKEN
	Name  = "MAX_Index_NOT_SENT_TO_BMC"
	Value  = "12"
	Help  = "Fast Video output Text Content"
	TokenType = Integer
	TargetMAK = Yes
	TargetH = Yes
End
############################################################################
#
# @Note: Set AC Power Loss Policy As Always On
# @Path: OemPkg\OemSetup\OemAcPowerOptSetup.sd
#
############################################################################
TOKEN
    Name  = "DEFAULT_ACLOSS_CONTROL"
    Value  = "2" ## 0 is Always Off,1(Default) is Last State,2 is Always On
    Help  = "Controls AC Loss state"
    TokenType = Integer
    TargetH = Yes
End
############################################################################
#
# @Note: Set Shell As Disabled In Default
# @Path: OemPkg\OemSetup\OemSetup.sd
#
############################################################################
TOKEN
    Name  = "DEFAULT_SHELL_SELECT_VALUE"
    Value  = "0"
    Help  = "0 - Disabled.\1 - Enabled."
    TokenType = Integer
    TargetEQU = Yes
    TargetMAK = Yes
    TargetH = Yes
End
