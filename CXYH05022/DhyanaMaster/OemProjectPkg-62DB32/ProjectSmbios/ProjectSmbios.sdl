	Name  = "ONBOARD_DEVICE_EXT_COUNT"
	Value  = "2"
	Help  = "Number of Onboard Devices"
	TokenType = Integer
	TargetEQU = Yes
	TargetH = Yes
	Range  = "0 - 16"
End

TOKEN
	Name  = "NUMBER_OF_ONBOARD_DEVICES"
	Value  = "2"
	Help  = "Number of Onboard Devices Present in the System."
	TokenType = Integer
	TargetEQU = Yes
	TargetH = Yes
	Range  = "0 - 16"
End
