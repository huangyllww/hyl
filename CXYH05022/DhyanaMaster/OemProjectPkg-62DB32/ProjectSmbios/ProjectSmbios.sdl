#SGEZT#29558:Sync SMBIOS Jumper Info->
TOKEN
    Name  = "NUMBER_OF_SYSTEM_CONFIG_STRINGS"
    Value  = "5"
    Help  = "Number of system configuration strings\identifies the total number of strings defined for this structure."
    TokenType = Integer
    TargetH = Yes
    TargetEQU = Yes
    Range  = "0-10"
End


TOKEN
