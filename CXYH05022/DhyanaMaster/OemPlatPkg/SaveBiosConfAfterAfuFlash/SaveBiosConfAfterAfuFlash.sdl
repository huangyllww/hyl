TOKEN
    Name  = "SaveBiosConfAfterAfuFlash_SUPPORT"
    Value  = "1"
    Help  = "Main switch to enable SaveBiosConfAfterAfuFlash support in Project"
    TokenType = Boolean
    TargetMAK = Yes
    Master = Yes
    Token = "Preserve_BIOS_Setting_OOB" "=" "1" 
