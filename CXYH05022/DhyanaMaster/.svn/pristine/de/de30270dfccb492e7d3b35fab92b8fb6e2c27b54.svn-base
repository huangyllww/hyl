#ifdef FORM_SET_ITEM

#define J37_USB_PORT0 \
    oneof varid = SETUP_DATA.J37USBPort0,\
      prompt      = STRING_TOKEN (STR_J37_USB_PORT0_PROMPT),\
      help        = STRING_TOKEN (STR_J37_USB_PORT0_PROMPT_HELP),\
      option text = STRING_TOKEN (STR_XHCI_PORT_ENABLED),        value = 1,    flags = DEFAULT | MANUFACTURING | RESET_REQUIRED;\
      option text = STRING_TOKEN (STR_XHCI_PORT_DISABLED),         value = 0,    flags = 0 | RESET_REQUIRED;\
    endoneof;
    
    #define J37_USB_PORT1 \
    oneof varid = SETUP_DATA.J37USBPort1,\
      prompt      = STRING_TOKEN (STR_J37_USB_PORT1_PROMPT),\
