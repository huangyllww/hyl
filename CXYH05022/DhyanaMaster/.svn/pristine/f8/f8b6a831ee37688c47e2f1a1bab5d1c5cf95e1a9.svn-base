  TokenType = Integer
  TargetH = Yes
End

TOKEN
  Name  = "FCH_CHK_SERIAL_CABLE"
  Value  = "0"
  Help  = "Skip serial output if no cable connected to save boot time."
  TokenType = Boolean
  TargetH = Yes   
End

TOKEN
  Name  = "OnBoardSerial_SUPPORT"
  Value  = "1"
  Help  = "For use of SoC UART."
  TokenType = Boolean
End

TOKEN
  Name  = "ExternalSerial_SUPPORT"
  Value  = "0"
  Help  = "Supports use of 1 LPC SIO 1 serial port debug card - 0x3F8."
  TokenType = Boolean
End

TOKEN
  Name  = "AmiSerialUart0_SUPPORT"
  Value  = "0"
  Help  = "Serial Protocol"
  TokenType = Boolean
  TargetEQU = Yes
  TargetMAK = Yes
  TargetH = Yes
End

TOKEN
  Name  = "AmiSerialUart1_SUPPORT"
  Value  = "1"
  Help  = "Serial Protocol"
  TokenType = Boolean
  TargetEQU = Yes
  TargetMAK = Yes
  TargetH = Yes
End

TOKEN
  Name  = "FCH_UART0_BASE_ADDRESS"
  Value  = "0xFEDC9000"
  Help  = "UART0 BASE ADDRESS"
  TokenType = Integer
  TargetH = Yes
End

TOKEN
  Name  = "FCH_UART1_BASE_ADDRESS"
  Value  = "0xFEDCA000"
  Help  = "UART1 BASE ADDRESS"
  TokenType = Integer
  TargetH = Yes
End

TOKEN
  Name  = "FCH_UART2_BASE_ADDRESS"
  Value  = "0xFEDCE000"
  Help  = "UART2 BASE ADDRESS"
  TokenType = Integer
  TargetH = Yes
End

TOKEN
  Name  = "FCH_UART3_BASE_ADDRESS"
  Value  = "0xFEDCF000"
  Help  = "UART3 BASE ADDRESS"
  TokenType = Integer
  TargetH = Yes
End

TOKEN
  Name  = "FCH_UART_BASE_ADDRESS"
  Value  = "$(FCH_UART1_BASE_ADDRESS)"
  Help  = "UART1 BASE ADDRESS"
  TokenType = Integer
  TargetH = Yes
End


#enable UART1 disable UART0
TOKEN
  Name  = "FchRTDeviceEnableMapToken"
  Value  = "0x1000"
  Help  = "1=Enabled on platform 0=disabled on platform\ UART0=bit11, UART1=bit12, UART2=bit16, UART3=bit26."
  TokenType = Integer
  TargetH = Yes
  Range  = "0...0FFFFh"
End

#set Uart0=Disabled
TOKEN
  Name  = "FchUart0LegacyEnableToken"
  Value  = "0"
  Help  = "0=Disabled 1=0x2E8 (COM4), 2=0x2F8 (COM2), 3=0x3E8 (COM3), 4=0x3F8 (COM1)"
  TokenType = Integer
  TargetH = Yes
  Range  = "0...0FFFFh"
End

#set Uart1=0x2F8
TOKEN
  Name  = "FchUart1LegacyEnableToken"
  Value  = "2"
  Help  = "0=Disabled 1=0x2E8 (COM4), 2=0x2F8 (COM2), 3=0x3E8 (COM3), 4=0x3F8 (COM1)"
  TokenType = Integer
  TargetH = Yes
  Range  = "0...0FFFFh"
End
