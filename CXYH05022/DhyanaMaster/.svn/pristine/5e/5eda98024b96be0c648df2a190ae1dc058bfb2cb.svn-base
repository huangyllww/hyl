End

INFComponent
	Name  = "DbgRxDbgrLib"
	File  = "DbgRxDbgrLib.inf"
	Package  = "AMIDebugRx"
End

LibraryMapping
	Class  = "CommonDbgrLib"
	Instance  = "AMIDebugRx.CommonDbgrLib"
End

LibraryMapping
	Class  = "DebugDbgrLib"
	Instance  = "AMIDebugRx.DbgRxDbgrLib"
End
