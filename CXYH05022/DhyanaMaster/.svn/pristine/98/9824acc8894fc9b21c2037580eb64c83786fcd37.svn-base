TOKEN
        Name  = "SmmSmbiosElog_SUPPORT"
        Value  = "1"
        Help  = "Main switch to enable SmmSmbiosElog support in Project"
        TokenType = Boolean
        TargetEQU = Yes
        TargetMAK = Yes
        TargetH = Yes
        Master = Yes
        Token = "SMM_SUPPORT" "=" "1"
End

INFComponent
        Name  = "SmmSmbiosElog"
        File  = "SmmSmbiosElog.inf"
        Package  = "AmiGpnvErrorLoggingPkg"
        ModuleTypes  = "DXE_SMM_DRIVER"
        Help  = "INF Component for SmmSmbiosElog driver"
End

PcdMapping
        Name = "PcdFixedDebugPrintErrorLevel"
        GuidSpace = "gEfiMdePkgTokenSpaceGuid"
        PcdType = "PcdsFixedAtBuild"
        Value = "0"
        Override = "AmiGpnvErrorLoggingPkg.SmmSmbiosElog"
        Offset = 00h
        Length =00h
        Help = "Disables all debug messages based on GPNV_MODULE_DEBUG_MESSAGES token value"
        TargetDsc = Yes
        Token = "GPNV_MODULE_DEBUG_MESSAGES" "=" "0"
        Token = "MDE_PKG_VERSION" ">=" "10"
End
