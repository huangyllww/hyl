#sideNav li a.selected.back::after {
    display: inline-block;
    position: absolute;
    top: calc(50% - (12px/2));
    right: 5px;
    content: ""; 
    width: 12px;
    height: 12px;
    transform: rotate(315deg);
}

body.dark #sideNav li a.selected.back::after {
    border-top: 1px solid #fff;
    border-left: 1px solid #fff;
}   

body.light #sideNav li a.selected.back::after {
    border-top: 1px solid #777;
    border-left: 1px solid #777;
}   

#sideNav nav.site {
    -moz-box-sizing: border-box;
