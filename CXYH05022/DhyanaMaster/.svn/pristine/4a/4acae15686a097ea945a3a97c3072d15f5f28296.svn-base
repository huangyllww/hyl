html,body{
  margin:0;
  padding:0;
  height: 100%;
}

body {
  font-family: Arial, Helvetica, sans-serif;    
  position: relative;
  -webkit-font-smoothing: antialiased;
  -webkit-user-select: none;  /* Chrome all / Safari all */
     -moz-user-select: none;  /* Firefox all */
      -ms-user-select: none;  /* IE 10+ */
      /* No support for these yet */
       -o-user-select: none;
          user-select: none;          
}

body.light {
 #background: #f4f4f4;
 #background: rgb(237, 237, 231);  
  background: rgb(241, 241, 237);     
  color: #999;
}

body.dark {
    #background: rgb(67, 69, 75);   /* #43454b */
    #background: rgb(74, 77, 78);   /* #4a4d4e */
    background: rgb(78, 80, 86);    /* #4e5056 */
    color: #eee;
}

h1 {
  font-weight: 600;
}

#wrapper {
  bottom: 0;
  left: 0;
  position: absolute;
  right: 0;
  top: 0;
}

#site {
  #width: 100%;
  #position: relative;
  bottom: 0;
  left: 0;
  position: absolute;
  right: 0;
  top: 0;
  z-index: 20;
  background: inherit;
  #left: 0;
  transition: all 0.2s ease-out;
  -webkit-transition: all 0.2s ease-out;
  transform: translate3d(0, 0, 0);
  -webkit-transform: translate3d(0, 0, 0);
}

#site:before{
  position: absolute;
  content: '';
#  left: -4px;
#  height: 100%;
#  width: 4px;
  background: transparent;
}

#site.open {
  transform: translate3d(230px, 0, 0);
  -webkit-transform: translate3d(230px, 0, 0);
   border-left: 2px solid #101010;
}

body.light #site.open {
  border-left: 2px solid #d0d0d0;
}

/* Main Header */
#main-header {
  color: #373435;
  background: #fff;
  height: 98px;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  padding: 10px 20px;
  position: relative;
}

body.dark #main-header {
  border-bottom: 3px solid #29cdfd;
}

body.light #main-header {
  border-bottom: 3px solid #d06515;
}

#main-header hgroup {
  text-align: center;
}

#main-header hgroup h1 {
  text-align: center;
  font-size: 30px;
  margin: auto ;
  position:relative;
  top:10px;
  letter-spacing: -.065em;
  line-height: 1.1em;
}

#main-header hgroup a {
  color: #777;
  text-decoration: none;
  display:inline-block;
  text-overflow: ellipsis;
  white-space: nowrap;
  max-width:calc(100% - 200px) ;
  overflow: hidden;
  padding:5px;        
}

#main-header hgroup a:hover {
  #color: #000;
}

#main-header hgroup p {
  font-size: 13px;
  color: #999;
  margin: 0;
}

#main-header nav {
  /* display: none; */
    display: block;
    position: absolute;
    right: 0;
    top: 0px;
    max-width: 600px;
    padding-top: 70px;
    padding-right: 20px;
}

#logo-button {
  position: absolute;
  top: 30px;
  right: 20px;
  display: inline-block;
}

#logo-button img {
    width:98px;
    height:25px;
  }  

#reset-button {
  position: absolute;
  top: 25px;
  right: 20px;
  display: inline-block;
}

  body.dark #reset-button {
    color: #29cdfd ;
    font-weight: bold;
  }
    
    body.light #reset-button {
    color: #d06515 ;
    font-weight: bold;
  }

#slide-menu-button {
  position: absolute;
  top: 20px;
  left: 20px;
  display: inline-block;
  vertical-align: top;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-background-clip: padding;
  -moz-background-clip: padding;
  background-clip: padding-box;
  padding: 0;
  margin: 0;
  font: inherit;
  color: inherit;
  background: transparent;
  cursor: default;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  padding: 0 0.5rem;
  line-height: 2rem;
  letter-spacing: 1px;
  color: #454545;
  text-shadow: 0 1px #fff;
  vertical-align: baseline;
  -webkit-box-shadow: inset 0 1px #fff;
  box-shadow: inset 0 1px #fff;
  -webkit-border-radius: 3px;
  border-radius: 3px;
  width: 2.6rem;
  height: 2.6rem;
  line-height: 2.6rem;
  border: 1px solid transparent;
  -webkit-box-shadow: none;
  box-shadow: none;
}

#slide-menu:disabled,
#slide-menu.is-disabled {
  opacity: 0.3;
  cursor: default;
  pointer-events: none;
}

#slide-menu-button:active,
#slide-menu-button.is-active {
  color: #454545;
  text-shadow: 0 1px #fff;
  background-color: #d3d7d7;
  border: 1px solid #a5a8a8;
  -webkit-box-shadow: inset 0 1px rgba(0,0,0,0.12);
  box-shadow: inset 0 1px rgba(0,0,0,0.12);
}

body.dark #slide-menu-button {
  fill: #29cdfd;
}

body.light #slide-menu-button {
  fill: #d06515;
}

#hamburger-menu-button {
    position: absolute;
    display: inline-block;
    top: 25px;
    left: 32px;
    width: 26px;  
    height: 16px;
    margin-top: 2px;
    padding: 0;
    background: rgba(0, 0, 0, 0) none repeat scroll 0 0;
    cursor: pointer;
}
      
#hamburger-menu-button .line {
    background: #ecf0f1 none repeat scroll 0 0;
    border-radius: 0;
    display: block;
    left: 0;
    position: absolute;
    transition: all 0.4s ease 0s;
}
      
#hamburger-menu-button .line {
    height: 4px;
    width: 27px;
}
      
#hamburger-menu-button .line.line-1 {
    top: 0;
}
 
#hamburger-menu-button .line.line-2 {
    top: 50%;
}
      
#hamburger-menu-button .line.line-3 {
    top: 100%;
}
          
#hamburger-menu-button.active .line-1 {
    transform: translateY(8px) translateX(0px) rotate(45deg);
}
      
#hamburger-menu-button.active .line-2 {
    opacity: 0;
}
      
#hamburger-menu-button.active .line-3 {
    transform: translateY(-8px) translateX(0px) rotate(-45deg);
}

body.dark #hamburger-menu-button .line {
    background: #29cdfd none repeat scroll 0 0 !important;
}
      
body.light #hamburger-menu-button .line {
    background: #d06515 none repeat scroll 0 0 !important;
}
      

/* Content */
#content {
  #width: 100%;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  padding: 20px;
    
    #border:5px red solid;
    #bottom: 0;
    bottom: 50px;
    left: 0px;
    position: absolute;
    right: 0;
    top: 100px;
    z-index: 4;
}

#content header h2 {
  font-weight: 300;
  margin: 10px 0 25px;
  font-size: 20px;
  position: relative;
  display: inline-block;
  padding-right: 10px;
}

body.light #content header h2 {
  background: inherit;
}

body.dark #content header h2 {
  #background: #4A4D4E;
    background: inherit;
}

#content header{
  position: relative;
}

#content header:before {
  content: '';
  width: 100%;
  display: block;
  position: absolute;
  left: 0;
  top: 23px;
}

body.light #content header:before {
  
}

#navigation_sidebar {
    -moz-user-select: none;
    bottom: 0;
    left: 0;
    overflow: hidden;
    position: absolute;
    top: 100px;
    width: 230px;
    box-sizing: border-box;
    display:none;
}

#footer {
    -moz-user-select: none;
    left:0px;
    right:0;
    bottom: 0;
    overflow: hidden;
    position: absolute;
    box-sizing: border-box;
    text-align:center;
    font-size: 12px;
}

#footer[role="contentinfo"] {
	padding: 5px 0 0 0;
	font-size: 12px;
	#border-top: 1px solid #ddd;
	}
#footer[role="contentinfo"] nav ul li {
	display: inline;
	margin: 0 10px 0 0;
	padding: 0 5px 0 0;
    color: #29cdfd;
	font-weight: bold;
	#border-right: 1px solid #ccc;
	}
#footer[role="contentinfo"] nav ul li:last-child {
	margin: 0;
	padding: 0;
	border: none;
	}
#footer[role="contentinfo"] p {
	margin: 10px 0;
	}			

footer p {
	margin: 10px 0;
	}				

body.dark footer p {
	color: #29cdfd;
}				

body.light footer p {
	color: #d06515 ;
}

footer nav {
    position: absolute;
    margin:0 5px 0 0 ;
    right: 0;
    top: 0;
}   
    
footer hgroup {
  text-align: center;
}   
    
    
footer #back-to-top {
  padding: 9px 2px 0 2px;
  background-color: #29cdfd;
  color: #fff;
  width: 25px;
  height: 25px;
  transition: all 1s ease 0s;
  cursor:pointer;    
}   
    
 footer #back-to-top::after { /* left thin arrow */
    display: inline-block;
    position: relative;
    content: ""; 
    width: 12px;
    height: 12px;
    margin:0px 0 0 0;
    transform: rotate(45deg);
}   

body.dark footer #back-to-top {
  background-color: #29cdfd;
  color: #fff;
}   

body.light footer #back-to-top {
  background-color: #d06515;
  color: #fff;
}   
    
    
body.dark footer #back-to-top::after {
    border-top: 2px solid #fff;
    border-left: 2px solid #fff;
}   

body.light footer #back-to-top::after {
    border-top: 2px solid #fff;
    border-left: 2px solid #fff;
  }  



body.dark #navigation_sidebar {
     border-right: 2px solid #101010;
   }    
    
body.light #navigation_sidebar {
    border-right: 2px solid #e0e0e0;
   }

#main-nav {
    bottom: 0px;
    left: 0;
    overflow-x: hidden;
    overflow-y: auto;
    position: absolute;
    right: -20px;
    top: 0px;
    margin-right: 20px;
}

#pageNav ul, #main-nav ul {
  list-style: none;
  margin: 0;
  margin-right: 0px;
  padding: 0 0 0 0;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

body.dark #main-nav li {
    border-bottom: 1px solid #0e0f10;
    border-top: 1px solid #717171;      
}

body.light #main-nav li {
    border-top: 2px solid #fdfdf8; 
    border-bottom: 1px solid #c2c2c2;
    background-color: #f1f1ed;  
}

#pageNav li a, #main-nav li a {
  display: block;
  #height: 46px;
  font-size: 18px;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  padding: 12px 0 0 20px;
  text-decoration: none;
}

#main-nav li a, #sideNav li a {
    color: #ffffff;
    min-height: 60px;
    padding: 20px ;
    position: relative;
    overflow: hidden;
    vertical-align: middle;
    text-overflow: ellipsis;
    white-space:pre-wrap ;
    font-weight: bold;
}

body.dark #main-nav li a, #sideNav li a {
    color: #fff;
}

body.light #main-nav li a, #sideNav li a {
    color: #777;
}

body.dark #main-nav li a:hover, body.dark #sideNav li a:hover {
    border-right: 1px solid #29cdfd;
}

body.light #main-nav li a:hover, body.light #sideNav li a:hover {
    border-right: 1px solid #d06515;
}

body.dark #main-nav li a.selected, body.dark #sideNav li a.selected {
    color: #29cdfd;
    border-right: 1px solid #29cdfd;
}

body.light #main-nav li a.selected, body.light #sideNav li a.selected {
    color: #d06515;
    border-right: 1px solid #d06515;
}

#main-nav li a.selected.back::after { /* left thin arrow */
    display: inline-block;
    position: absolute;
    top: calc(50% - (15px/2));
    right: 5px;
    content: ""; 
    width: 15px;
    height: 15px;
    transform: rotate(315deg);
}   

body.dark #main-nav li a.selected.back::after {
    border-top: 1px solid #fff;
    border-left: 1px solid #fff;
}   

body.light #main-nav li a.selected.back::after {
    border-top: 1px solid #777;
    border-left: 1px solid #777;
  }   

/* Side Nav */
#sideNav {
  -moz-user-select: none;
   bottom: 0;
   left: 0;
   overflow: hidden;
   position: absolute;
   top: 0;
   width: 230px;
   box-sizing: border-box;
   #border:5px blue solid;
   #display:none;
}

#sideNav ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

body.dark #sideNav li {
 color: #FFFFFF;    
 border-top: 1px solid #717171;      
 border-bottom: 1px solid #0e0f10;
}

body.light #sideNav li {
 color: #888;
 border-top: 1px solid #fdfdf8; 
 border-bottom: 1px solid #c2c2c2;  
}

#sideNav li a {
    color: inherit;
    display: block;
    min-height: 46px;
    font-size: 16px;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    padding: 22px ;
    position: relative;
    overflow: hidden;
    vertical-align: middle;
    text-overflow: ellipsis;
    white-space:pre-wrap ;
    font-weight: bold;
    text-decoration: none;
}

