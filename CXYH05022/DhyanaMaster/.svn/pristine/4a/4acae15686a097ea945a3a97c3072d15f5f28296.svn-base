.switch,
.topcoat-switch {
  position: relative;
  display: inline-block;
  vertical-align: top;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  background-clip: padding-box;
}

.switch__input,
.topcoat-switch__input {
  position: absolute;
  overflow: hidden;
  padding: 0;
  border: 0;
  opacity: 0.001;
  z-index: 1;
  vertical-align: top;
  outline: none;
}

.switch__toggle,
.topcoat-switch__toggle {
  position: relative;
  display: inline-block;
  vertical-align: top;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  background-clip: padding-box;
  padding: 0;
  margin: 0;
  font: inherit;
  color: inherit;
  background: transparent;
  border: none;
  cursor: default;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}

.switch__toggle:before,
.switch__toggle:after,
.topcoat-switch__toggle:before,
.topcoat-switch__toggle:after {
  content: '';
  position: absolute;
  z-index: -1;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  background-clip: padding-box;
}

.switch--disabled,
.topcoat-switch__input:disabled + .topcoat-switch__toggle {
  opacity: 0.3;
  cursor: default;
  pointer-events: none;
}


.topcoat-switch {
  font-size: 12px;
  padding: 0 0.563rem;
  border-radius: 4px;
  border: 1px solid #333434;
  overflow: hidden;
  width: 3.5rem;
  #width: 5.0rem;    
}

body.dark .topcoat-switch {
  border: 1px solid #333434;
 }

body.light .topcoat-switch {
  border: 1px solid #aaaaaa;
 }


.topcoat-switch__toggle:before,
.topcoat-switch__toggle:after {
  top: -1px;
  width: 2.6rem;
}

.topcoat-switch__toggle:before {
  content: 'On';
  color: #29cdfd;
  background-color: #3f4041;
  right: 0.8rem;
  #right: 3.9rem;
  padding-left: 0.75rem;
  #padding-left: 2.0rem;
}

body.dark .topcoat-switch__toggle:before {
  color: #29cdfd;
  background-color: #3f4041;
}

body.light .topcoat-switch__toggle:before {
  color: #d06515;
  background-color: #fff;
}


.topcoat-switch__toggle {
  line-height: 1.313rem;
  height: 1.313rem;
  width: 1rem;
  border-radius: 4px;
  color: #c6c8c8;
  text-shadow: 0 -1px rgba(0,0,0,0.69);
  background-color: #595b5b;
  border: 1px solid #333434;
  margin-left: -0.6rem;
  margin-bottom: -1px;
  margin-top: -1px;
  box-shadow: inset 0 1px #737373;
  -webkit-transition: margin-left 0.05s ease-in-out;
  transition: margin-left 0.05s ease-in-out;
}

body.dark .topcoat-switch__toggle {
  color: #c6c8c8;
  text-shadow: 0 -1px rgba(0,0,0,0.69);
  background-color: #595b5b;
  border: 1px solid #333434;
  box-shadow: inset 0 1px #737373;
}

body.light .topcoat-switch__toggle {
  color: #777;
  text-shadow: 0 -1px #fff;
  #background-color: #e5e9e8;
  background-color:#d4d0c8;
  border: 1px solid #aaaaaa;
  box-shadow: inset 0 1px #fff;
}

.topcoat-switch__toggle:after {
  content: 'Off';
  background-color: #3f4041;
  left: 0.8rem;
  padding-left: 0.6rem;
}

body.dark .topcoat-switch__toggle:after {
  background-color: #3f4041;
}

body.light .topcoat-switch__toggle:after {
  #background-color: #d2d6d6;
  background-color: #efefef;
}


.topcoat-switch__input:checked + .topcoat-switch__toggle {
  margin-left: 1.85rem;
  #margin-left: 3.4rem;
}

.topcoat-switch__input:active + .topcoat-switch__toggle {
  border: 1px solid #333434;
  box-shadow: inset 0 1px #737373;
}

body.dark .topcoat-switch__input:active + .topcoat-switch__toggle {
  border: 1px solid #333434;
  box-shadow: inset 0 1px #737373;
}

body.light .topcoat-switch__input:active + .topcoat-switch__toggle {
  border: 1px solid #9daca9;
  box-shadow: inset 0 1px #fff;
}

.topcoat-switch__input:focus + .topcoat-switch__toggle {
  border: 1px solid #29cdfd;
  box-shadow: 0 0 0 1px #29cdfd;
}

body.light .topcoat-switch__input:focus + .topcoat-switch__toggle {
  border: 1px solid #d06515;
  box-shadow: 0 0 0 1px #d06515;
}


.topcoat-switch__input:disabled + .topcoat-switch__toggle:after,
.topcoat-switch__input:disabled + .topcoat-switch__toggle:before {
  background: transparent;
}

