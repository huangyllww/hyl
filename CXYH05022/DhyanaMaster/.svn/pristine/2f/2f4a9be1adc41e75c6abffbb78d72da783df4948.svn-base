TOKEN
	Name  = "OemSmbios_SUPPORT"
	Value  = "1"
	Help  = "Main switch to enable Oem Smbios support in Project"
	TokenType = Boolean
	TargetEQU = Yes
	TargetMAK = Yes
	Master = Yes
End

PATH
	Name  = "SUGOMSMBIOS_DIR"
End

TOKEN
    Name  = "OemPkgSmbios_SUPPORT"
    Value  = "0"
    Help  = "Main switch to enable Oem Smbios support in Project"
    TokenType = Boolean
    TargetEQU = Yes
    TargetMAK = Yes
    Token = "OemSmbios_SUPPORT" "=" "1"
End


