#
#################################################################################
TOKEN
	Name  = "RESTORE_PASSWORD_ON_CMOS_CLEAR"
	Value  = "0"
	Help  = "Switch for Enabling OemPkgDxe support in the project"
	TargetH = Yes
	TokenType = Boolean
End

ELINK
  Name  = "gOemPkgTokenSpaceGuid.PcdCmosClear"
  Parent  = "AdditionalOemDxeHookPcds"
  InvokeOrder = AfterParent
