#dengyh secure boot 
TOKEN
    Name  = "SecureBoot_SUPPORT"
    Value  = "1"
    Help  = "Main switch to enable SecureBoot Module support in Project"
    TokenType = Boolean
    TargetH = Yes
    Token = "AmiCryptoPkg_SUPPORT" "=" "1" 
    Token = "NVRAM_VERSION" ">=" "8"
End


