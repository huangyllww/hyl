End

ELINK
    Name  = "OemPkg/OemPkg.dec"
    Parent  = "AdditionalSetupPackages"
    InvokeOrder = AfterParent    
End

ELINK
    Name  = "ITEM_CALLBACK(ADVANCED_FORM_SET_CLASS,0,POST_EX_KEY,SetPOSTEXCallBack),"
    Parent  = "SetupItemCallbacks"
    InvokeOrder = AfterParent
    Token = "VGA_UART_DIS16BIT_PORT80_SUPPORT" "=" "1"
End
