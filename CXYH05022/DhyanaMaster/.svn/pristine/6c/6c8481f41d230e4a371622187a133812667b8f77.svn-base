TOKEN
    Name  = "OemDebugAlways_SUPPORT"
    Value  = "1"
    Help  = "Main switch to enable OemDebugAlways support in Project"
    TokenType = Boolean
    TargetH = Yes
    Master = Yes
End

TOKEN
    Name  = "OemDebugAlwaysSrc_SUPPORT"
    Value  = "0"
    Help  = "Main switch to enable OemDebugAlways support in Project"
    TokenType = Boolean
    TargetH = Yes
End

PATH
    Name  = "OEM_DEBUG_ALWAYS_IDR"
End

ELINK
    Name  = "$(OEM_DEBUG_ALWAYS_IDR)\OemDebugAlways.sd"
    Parent  = "SETUP_DEFINITIONS"
    InvokeOrder = AfterParent
End

ELINK
    Name  = "$(OEM_DEBUG_ALWAYS_IDR)\OemDebugAlways.uni"
    Parent  = "SetupStringFiles"
    InvokeOrder = AfterParent
End

ELINK
    Name  = "$(OEM_DEBUG_ALWAYS_IDR)\OemDebugAlways.ssp"
    Parent  = "ADDON_SSP_FILES"
    InvokeOrder = AfterParent
End

TOKEN
    Name  = "FAST_VIDEO_ENABLE"
    Value  = "1"
    Help  = "AST parent bridge Function Number"
    TokenType = Integer
    TargetMAK = Yes
    TargetH = Yes
End

TOKEN
    Name  = "FAST_VIDEO_TEXT_MODE"
    Value  = "0"
    Help  = "AST parent bridge Function Number"
    TokenType = Integer
    TargetMAK = Yes
    TargetH = Yes
End

TOKEN
    Name  = "CMOS_DEBUG_MODE_OFFSET"
    Value  = "0x46"
    Help  = "Modify the value."
    TokenType = Integer
    TargetEQU = Yes
    TargetMAK = Yes
    TargetH = Yes
End

TOKEN
    Name  = "DEFAULT_DEBUG_MODE_VALUE"
    Value  = "0"
    Help  = "0 - Default disable debug function.\1 - PEI debug enable.\2 - DXE debug enable.\8 - MRC debug enable.\0xF - ALL debug enable."
    TokenType = Integer
    TargetEQU = Yes
    TargetMAK = Yes
    TargetH = Yes
    Token = "OemProduction_BIOS" "=" "1"
End

TOKEN
    Name  = "DEFAULT_DEBUG_MODE_VALUE"
    Value  = "0x0F"
    Help  = "0 - Default disable debug function.\1 - PEI debug enable.\2 - DXE debug enable.\8 - MRC debug enable.\0xF - ALL debug enable."
    TokenType = Integer
    TargetEQU = Yes
    TargetMAK = Yes
    TargetH = Yes
    Token = "OemProduction_BIOS" "=" "0"
End
