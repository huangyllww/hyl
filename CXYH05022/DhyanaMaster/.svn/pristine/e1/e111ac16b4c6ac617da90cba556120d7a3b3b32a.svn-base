TOKEN
    Name  = "SOC2_PSP_SUPPORT"
    Value  = "1"
    Help  = "Main switch to enable SOC#1 support in Project"
    TokenType = Boolean
    TargetEQU = Yes
    TargetMAK = Yes
    TargetH = Yes
    Master = Yes
    Token = "MULTI_CPU_SUPPORT" "=" "1"
End

#********************************** CSV & TPCM ***********************************# 
