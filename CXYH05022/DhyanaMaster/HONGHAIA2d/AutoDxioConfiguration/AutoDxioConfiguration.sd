#ifdef ADVANCED_FORM_SET

//-------------------------------------------------------------------------
//Define controls to be added to the top level page of the formset
//-------------------------------------------------------------------------
#ifdef FORM_SET_ITEM

#endif

#ifdef FORM_SET_GOTO
      // Define goto commands for the forms defined in this file
      goto STR_DXIO_CONFIG_FORM_ID,
      prompt = STRING_TOKEN(STR_DXIO_CONFIG_FORM),
      help = STRING_TOKEN(STR_DXIO_CONFIG_FORM_HELP);   
#endif

#ifdef FORM_SET_FORM

      form formid = AUTO_ID(STR_DXIO_CONFIG_FORM_ID),
      title = STRING_TOKEN(STR_DXIO_CONFIG_FORM);
  
      SUBTITLE(STRING_TOKEN(STR_DXIO_CONFIG_FORM_ID))
      SEPARATOR
          oneof varid = SETUP_DATA.EnableDxioCfg,\
                prompt  = STRING_TOKEN(STR_ENABLE_DXIO_CONFIG),\
                help    = STRING_TOKEN(STR_ENABLE_DXIO_CONFIG_HELP),\
                option text = STRING_TOKEN(STR_CFG_DISABLE), value = 0, flags = DEFAULT | MANUFACTURING | RESET_REQUIRED;\
                option text = STRING_TOKEN(STR_CFG_ENABLE),  value = 1, flags = RESET_REQUIRED;\
          endoneof; 
          suppressif ideqval SETUP_DATA.EnableDxioCfg == 0;    
              oneof varid = SETUP_DATA.DieDxioType0Conf[0],\
                    prompt  = STRING_TOKEN(STR_DIE0_DXIO_TYPE_A_CFG),\
                    help    = STRING_TOKEN(STR_DIE0_DXIO_TYPE_A_CFG_HELP),\
                    option text = STRING_TOKEN(STR_NORMAL),     value = 0, flags = DEFAULT | MANUFACTURING | RESET_REQUIRED;\
                    option text = STRING_TOKEN(STR_PNS_CARD), value = 1, flags = RESET_REQUIRED;\
                    option text = STRING_TOKEN(STR_2SFP_2SGMII), value = 2, flags = RESET_REQUIRED;\
                    option text = STRING_TOKEN(STR_4SGMII), value = 3, flags = RESET_REQUIRED;\
                    option text = STRING_TOKEN(STR_KR_BCM84328), value = 4, flags = RESET_REQUIRED;\
                    option text = STRING_TOKEN(STR_KR_inphi4233), value = 5, flags = RESET_REQUIRED;\
              endoneof;              
               
          endif;            
    endform;
#endif
#endif // ADVANCED_FORM_SET
