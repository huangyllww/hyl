TOKEN
    Name  = "AUTO_DXIO_CONFIGURATION_SUPPORT_52DB16"
    Value  = "1"
    Help  = "Main switch to enable auto configure DXIO in Project"
    TokenType = Boolean
    TargetEQU = Yes
    TargetMAK = Yes
    Master = Yes
End

PATH
    Name  = "AUTO_DXIO_CONFIGURATION_DIR"
End

INFComponent
    Name  = "AutoDxioConfiguration"
    File  = "AutoDxioConfiguration.inf"
    Package  = "OemProjectPkg-52DB16"
    ModuleTypes  = "PEIM"
End

ELINK
    Name  = "$(AUTO_DXIO_CONFIGURATION_DIR)\PciSataSwitch.sd"
    Parent  = "SETUP_DEFINITIONS"
    InvokeOrder = AfterParent    
End

ELINK
    Name  = "$(AUTO_DXIO_CONFIGURATION_DIR)\PciSataSwitch.uni"
    Parent  = "SetupStringFiles"
    InvokeOrder = AfterParent
End
