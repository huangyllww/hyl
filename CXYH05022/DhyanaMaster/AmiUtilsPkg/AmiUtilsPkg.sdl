    TokenType = Boolean
    TargetMAK = Yes
    TargetH = Yes
End

TOKEN
    Name  = "ConditionalExpression_INF_SUPPORT"
    Value  = "1"
    Help  = "Main switch to enable ConditionalExpression support in Project"
    TokenType = Boolean
End

