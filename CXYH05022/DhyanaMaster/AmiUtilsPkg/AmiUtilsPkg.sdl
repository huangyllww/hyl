    Value  = "1"
    Help  = "Main switch to enable AmiUtilsPkg"
    TokenType = Boolean
    TargetEQU = Yes
    TargetMAK = Yes
    Master = Yes
End

TOKEN
    Name  = "SUPPRESS_COND_EXPRESSION_CONTROLS"
    Value  = "0"
