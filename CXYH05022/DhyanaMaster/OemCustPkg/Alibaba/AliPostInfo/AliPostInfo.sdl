#<lvych001-20170417 Improve post message display function.+>
TOKEN
  Name  = "TSE_POSTSCREEN_SCROLL_AREA"
  Value  = "{0,0,30,100}"
  Help  = "Format:{TopRow, TopCol, BottomRow, BottomCol}. Used to Configure the TSE PostScreen Scroll Area. Note Post message are still printed in whole Screen."
  TokenType = Expression
  TargetH = Yes
  Token = "GC_INITIAL_MODE" "=" "2"
End

TOKEN
    Name  = "GC_INITIAL_MODE"
    Value  = "2"
    Help  = "This value is an index into the the GC_MODE_LIST array token that indicates\ the text mode that the system will be in when the system starts"
    TokenType = Integer
    TargetH = Yes
End
