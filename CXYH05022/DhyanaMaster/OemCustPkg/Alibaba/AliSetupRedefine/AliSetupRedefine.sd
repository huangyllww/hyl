#ifdef CONTROL_DEFINITION
#if defined(ALI_DMI_VENDOR_CHANGE_SUPPORT)&& (ALI_DMI_VENDOR_CHANGE_SUPPORT == 1)
#define ALI_DMI_VENDOR_CHANGE\
    oneof varid     = SETUP_DATA.DmiVendorChange,\
        prompt      = STRING_TOKEN(STR_ALI_DMI_VENDOR_CHANGE), \
        help        = STRING_TOKEN(STR_ALI_DMI_VENDOR_CHANGE_HELP), \
        default = DEFAULT_ALI_DMI_VENDOR_CHANGE,\
        option text = STRING_TOKEN(STR_DISABLE),    value = 0, flags = RESET_REQUIRED; \
        option text = STRING_TOKEN(STR_ENABLE),    value = 1, flags = RESET_REQUIRED; \
    endoneof;
#endif

#if defined(ALI_LOCAL_HDD_BOOT_SUPPORT)&& (ALI_LOCAL_HDD_BOOT_SUPPORT == 1)
#define ALI_LOCAL_HDD_BOOT\
    oneof varid     = SETUP_DATA.LocalHddBoot,\
        prompt      = STRING_TOKEN(STR_ALI_LOCAL_HDD_BOOT), \
        help        = STRING_TOKEN(STR_ALI_LOCAL_HDD_BOOT_HELP), \
        default = DEFAULT_ALI_LOCAL_HDD_BOOT,\
        option text = STRING_TOKEN(STR_DISABLE),    value = 0, flags = RESET_REQUIRED; \
        option text = STRING_TOKEN(STR_ENABLE),    value = 1, flags = RESET_REQUIRED; \
    endoneof;
#endif
#endif //CONTROL_DEFINITION

#ifdef BOOT_FORM_SET
